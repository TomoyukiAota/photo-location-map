{
  "name": "photo-location-map",
  "productName": "Photo Location Map",
  "version": "0.4.1-alpha.0",
  "description": "Display the locations where your photos are taken on a map.",
  "homepage": "https://github.com/TomoyukiAota/photo-location-map",
  "author": {
    "name": "Tomoyuki Aota"
  },
  "main": "src-main/electron-main.js",
  "private": true,
  "config": {
    "devOrProd": "Dev"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p ng:serve electron:serve",
    "build": "npm run electron:serve-tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && electron-builder build --linux",
    "electron:windows": "npm run build:prod && electron-builder build --windows",
    "electron:mac": "npm run build:prod && electron-builder build --mac",
    "publish:linux": "npm run electron:linux -- --publish always",
    "publish:windows": "npm run electron:windows -- --publish always",
    "publish:mac": "npm run electron:mac -- --publish always",
    "notarize-history": "node script/notarize/get-notarization-history.js",
    "lint:ng": "ng lint",
    "lint:es": "eslint ./script/**/*.js",
    "lint:es:fix": "eslint --fix ./script/**/*.js",
    "lint:all": "npm-run-all lint:ng lint:es",
    "test:unit:karma": "ng test --code-coverage",
    "test:unit:karma:watch": "ng test --code-coverage --watch",
    "test:unit:em:main": "cross-env TS_NODE_TRANSPILE_ONLY=1 electron-mocha --config ./test/electron-mocha-main.mocharc.js",
    "test:unit:em:renderer": "cross-env TS_NODE_TRANSPILE_ONLY=1 electron-mocha --opts ./test/electron-mocha-renderer.opts",
    "test:unit:em:renderer:not-working": "cross-env TS_NODE_TRANSPILE_ONLY=1 electron-mocha --renderer --config ./test/electron-mocha-renderer.mocharc.js",
    "test:unit:em:all": "npm-run-all test:unit:em:main test:unit:em:renderer",
    "test:unit:all": "npm-run-all test:unit:karma test:unit:em:all",
    "test:e2e": "npm run build:prod && mocha --config ./e2e/e2e.mocharc.js",
    "test:package": "cross-env PLM_PACKAGE_TEST=true node ./script/package-test/run-package-test.js",
    "test:all": "npm-run-all lint:all test:unit:all test:e2e test:package",
    "check": "npm-check --skip-unused --save-exact --update",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "dependencies": {
    "electron-updater": "4.3.5",
    "electron-window-state": "5.0.3",
    "fs-extra": "9.0.1",
    "is-natural-number": "4.0.1",
    "moment-timezone": "0.5.26",
    "universal-analytics": "0.4.20",
    "uuid": "8.3.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "10.0.1",
    "@angular-devkit/build-angular": "0.1001.6",
    "@angular/animations": "10.1.5",
    "@angular/cdk": "10.2.4",
    "@angular/cli": "10.1.6",
    "@angular/common": "10.1.5",
    "@angular/compiler": "10.1.5",
    "@angular/compiler-cli": "10.1.5",
    "@angular/core": "10.1.5",
    "@angular/forms": "10.1.5",
    "@angular/language-service": "10.1.5",
    "@angular/material": "10.2.4",
    "@angular/platform-browser": "10.1.5",
    "@angular/platform-browser-dynamic": "10.1.5",
    "@angular/router": "10.1.5",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@types/fs-extra": "9.0.2",
    "@types/is-natural-number": "4.0.0",
    "@types/jasmine": "3.5.14",
    "@types/jasminewd2": "2.0.8",
    "@types/leaflet": "1.5.18",
    "@types/leaflet.markercluster": "1.4.3",
    "@types/moment-timezone": "0.5.12",
    "@types/node": "12.11.6",
    "@types/universal-analytics": "0.4.3",
    "@types/uuid": "8.3.0",
    "@types/webdriverio": "4.13.3",
    "@types/webpack": "4.41.22",
    "chai": "4.2.0",
    "codelyzer": "6.0.1",
    "conventional-changelog-cli": "2.1.0",
    "cross-env": "5.2.0",
    "devtron": "1.4.0",
    "directory-tree": "2.2.5",
    "electron": "9.3.2",
    "electron-builder": "22.8.1",
    "electron-mocha": "8.2.2",
    "electron-notarize": "1.0.0",
    "electron-reload": "1.5.0",
    "eslint": "6.2.2",
    "espower-typescript": "9.0.2",
    "exif-parser": "0.1.12",
    "hammerjs": "2.0.8",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "5.2.3",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "6.3.1",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "karma-mocha-reporter": "2.2.5",
    "npm-check": "5.9.2",
    "npm-run-all": "4.1.5",
    "power-assert": "1.6.1",
    "rxjs": "6.6.3",
    "spectron": "11.0.0",
    "split.js": "1.6.2",
    "tree-kill": "1.2.2",
    "ts-node": "8.3.0",
    "tslib": "2.0.3",
    "tslint": "6.1.3",
    "typescript": "4.0.3",
    "wait-on": "3.3.0",
    "zone.js": "0.10.3"
  },
  "browser": {
    "fs": false,
    "net": false,
    "tls": false
  }
}
