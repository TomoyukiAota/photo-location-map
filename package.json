{
  "name": "photo-location-map",
  "productName": "Photo Location Map",
  "version": "1.1.1-alpha",
  "description": "Display the locations where your photos are taken on a map.",
  "homepage": "https://github.com/TomoyukiAota/photo-location-map",
  "author": {
    "name": "Tomoyuki Aota"
  },
  "main": "src-main/electron-main.js",
  "private": true,
  "config": {
    "devOrProd": "Dev"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p ng:serve electron:serve",
    "build": "npm run electron:serve-tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron-builder:linux": "npm run build:prod && electron-builder build --linux",
    "electron-builder:mac": "npm run build:prod && electron-builder build --mac",
    "electron-builder:windows": "npm run build:prod && electron-builder build --windows",
    "package:linux": "npm run electron-builder:linux -- --publish never",
    "package:mac": "npm run electron-builder:mac -- --publish never",
    "package:windows": "npm run electron-builder:windows -- --publish never",
    "publish:linux": "npm run electron-builder:linux -- --publish always",
    "publish:mac": "npm run electron-builder:mac -- --publish always",
    "publish:windows": "npm run electron-builder:windows -- --publish always",
    "notarize-history": "node script/notarize/get-notarization-history.js",
    "lint:ng": "ng lint",
    "lint:es": "eslint ./script/**/*.js",
    "lint:es:fix": "eslint --fix ./script/**/*.js",
    "lint:all": "npm-run-all lint:ng lint:es",
    "test:unit:ng": "ng test",
    "test:unit:ng:watch": "ng test --watch",
    "test:unit:ng:coverage": "ng test --code-coverage",
    "test:unit:em:main": "cross-env TS_NODE_TRANSPILE_ONLY=1 PLM_EM_MAIN=true electron-mocha --config ./test/em-main.mocharc.js",
    "test:unit:em:renderer": "cross-env TS_NODE_TRANSPILE_ONLY=1 PLM_EM_RENDERER=true electron-mocha --renderer --require-main ./test/em-renderer-require-in-main-process.js --window-config ./test/em-renderer-window-config.js --config ./test/em-renderer.mocharc.js",
    "test:unit:em:both": "npm-run-all test:unit:em:main test:unit:em:renderer",
    "test:unit:em:help": "electron-mocha --help",
    "test:unit:all": "npm-run-all test:unit:ng test:unit:em:both",
    "test:package": "cross-env PLM_PACKAGE_TEST=true node ./script/package-test/run-package-test.js",
    "test:all": "npm-run-all lint:all test:unit:all test:package",
    "check": "npm-check --skip-unused --save-exact --update"
  },
  "dependencies": {
    "@electron/remote": "1.1.0",
    "delete-empty": "3.0.0",
    "electron-unhandled": "3.0.2",
    "electron-updater": "4.3.9",
    "electron-window-state": "5.0.3",
    "exifr": "7.0.0",
    "fs-extra": "10.0.0",
    "heic-convert": "1.2.4",
    "is-natural-number": "4.0.1",
    "moment-timezone": "0.5.33",
    "nodejs-fs-utils": "1.2.5",
    "physical-cpu-count": "2.0.0",
    "prettysize": "2.0.0",
    "promise.allsettled": "1.0.4",
    "threads": "1.6.4",
    "tiny-worker": "2.3.0",
    "universal-analytics": "0.4.23",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "12.1.1",
    "@angular-devkit/build-angular": "12.2.3",
    "@angular/animations": "12.2.3",
    "@angular/cdk": "12.2.3",
    "@angular/cli": "12.2.3",
    "@angular/common": "12.2.3",
    "@angular/compiler": "12.2.3",
    "@angular/compiler-cli": "12.2.3",
    "@angular/core": "12.2.3",
    "@angular/forms": "12.2.3",
    "@angular/language-service": "12.2.3",
    "@angular/material": "12.2.3",
    "@angular/platform-browser": "12.2.3",
    "@angular/platform-browser-dynamic": "12.2.3",
    "@angular/router": "12.2.3",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@types/delete-empty": "3.0.0",
    "@types/fs-extra": "9.0.11",
    "@types/is-natural-number": "4.0.0",
    "@types/jasmine": "3.6.11",
    "@types/leaflet": "1.7.0",
    "@types/leaflet.markercluster": "1.4.4",
    "@types/lodash": "4.14.165",
    "@types/node": "12.20.13",
    "@types/promise.allsettled": "1.0.3",
    "@types/universal-analytics": "0.4.4",
    "@types/uuid": "8.3.0",
    "@types/webpack": "5.28.0",
    "chai": "4.3.4",
    "codelyzer": "6.0.2",
    "cross-env": "7.0.3",
    "directory-tree": "2.2.9",
    "electron": "12.0.9",
    "electron-builder": "22.10.5",
    "electron-mocha": "10.0.0",
    "electron-notarize": "1.0.0",
    "electron-reload": "1.5.0",
    "eslint": "7.27.0",
    "espower-typescript": "10.0.0",
    "exif-parser": "0.1.12",
    "hammerjs": "2.0.8",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "6.3.4",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "7.0.0",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "karma-mocha-reporter": "2.2.5",
    "ng-mocks": "12.4.0",
    "npm-check": "5.9.2",
    "npm-run-all": "4.1.5",
    "power-assert": "1.6.1",
    "rxjs": "6.6.7",
    "split.js": "1.6.4",
    "tree-kill": "1.2.2",
    "ts-node": "8.3.0",
    "tslib": "2.2.0",
    "tslint": "6.1.3",
    "typescript": "4.3.5",
    "wait-on": "5.3.0",
    "zone.js": "0.11.4"
  },
  "browser": {
    "fs": false,
    "net": false,
    "tls": false
  }
}
