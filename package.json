{
  "name": "photo-location-map",
  "productName": "Photo Location Map",
  "version": "0.2.0-alpha.0",
  "description": "Display the locations where your photos are taken on a map.",
  "homepage": "https://github.com/TomoyukiAota/photo-location-map",
  "author": {
    "name": "Tomoyuki Aota"
  },
  "main": "src-main/electron-main.js",
  "private": true,
  "config": {
    "devOrProd": "Dev"
  },
  "scripts": {
    "postinstall": "npm run postinstall:electron && electron-builder install-app-deps",
    "postinstall:web": "node postinstall-web",
    "postinstall:electron": "node postinstall",
    "ng": "ng",
    "start": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "build": "npm run postinstall:electron && npm run electron:serve-tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve",
    "ng:serve:web": "npm run postinstall:web && ng serve -o",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && electron-builder build --linux",
    "electron:windows": "npm run build:prod && electron-builder build --windows",
    "electron:mac": "npm run build:prod && electron-builder build --mac",
    "publish:linux": "npm run electron:linux -- --publish always",
    "publish:windows": "npm run electron:windows -- --publish always",
    "publish:mac": "npm run electron:mac -- --publish always",
    "notarize-history": "node script/notarize/get-notarization-history.js",
    "lint:ng": "ng lint",
    "lint:es": "eslint ./script/**/*.js",
    "lint:es:fix": "eslint --fix ./script/**/*.js",
    "lint:all": "npm-run-all lint:ng lint:es",
    "test:unit:karma": "npm run postinstall:electron && ng test --code-coverage",
    "test:unit:karma:watch": "npm run postinstall:electron && ng test --code-coverage --watch",
    "test:unit:em:main": "cross-env TS_NODE_TRANSPILE_ONLY=1 electron-mocha --opts ./test/electron-mocha-main.opts",
    "test:unit:em:renderer": "cross-env TS_NODE_TRANSPILE_ONLY=1 electron-mocha --opts ./test/electron-mocha-renderer.opts",
    "test:unit:em:all": "npm-run-all test:unit:em:main test:unit:em:renderer",
    "test:unit:all": "npm-run-all test:unit:karma test:unit:em:all",
    "test:e2e": "npm run build:prod && mocha --opts ./e2e/mocha-e2e.opts",
    "test:package": "cross-env PLM_PACKAGE_TEST=true node ./script/package-test/run-package-test.js",
    "test:all": "npm-run-all lint:all test:unit:all test:e2e test:package",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "dependencies": {
    "electron-updater": "4.1.2",
    "electron-window-state": "5.0.3",
    "fs-extra": "8.1.0",
    "jimp": "0.6.4",
    "moment-timezone": "0.5.26",
    "universal-analytics": "0.4.20",
    "uuid": "3.3.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.802.2",
    "@angular/animations": "8.2.4",
    "@angular/cdk": "8.1.4",
    "@angular/cli": "8.3.1",
    "@angular/common": "8.2.4",
    "@angular/compiler": "8.2.4",
    "@angular/compiler-cli": "8.2.4",
    "@angular/core": "8.2.4",
    "@angular/forms": "8.2.4",
    "@angular/language-service": "8.2.4",
    "@angular/material": "8.1.4",
    "@angular/platform-browser": "8.2.4",
    "@angular/platform-browser-dynamic": "8.2.4",
    "@angular/router": "8.2.4",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@types/fs-extra": "8.0.0",
    "@types/jasmine": "3.4.0",
    "@types/jasminewd2": "2.0.6",
    "@types/leaflet": "1.5.1",
    "@types/leaflet.markercluster": "1.4.0",
    "@types/moment-timezone": "0.5.12",
    "@types/node": "12.11.6",
    "@types/universal-analytics": "0.4.3",
    "@types/uuid": "3.4.5",
    "@types/webdriverio": "4.13.3",
    "angular-split": "3.0.2",
    "chai": "4.2.0",
    "codelyzer": "5.0.1",
    "conventional-changelog-cli": "2.0.23",
    "core-js": "3.2.1",
    "cross-env": "5.2.0",
    "devtron": "1.4.0",
    "directory-tree": "2.2.4",
    "electron": "7.0.0",
    "electron-builder": "21.2.0",
    "electron-mocha": "8.1.2",
    "electron-notarize": "0.2.0",
    "electron-reload": "1.5.0",
    "eslint": "6.2.2",
    "espower-typescript": "9.0.2",
    "exif-parser": "0.1.12",
    "hammerjs": "2.0.8",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.3.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-electron": "6.3.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "karma-mocha-reporter": "2.2.5",
    "npm-run-all": "4.1.5",
    "power-assert": "1.6.1",
    "rxjs": "6.5.2",
    "spectron": "9.0.0",
    "tree-kill": "1.2.1",
    "ts-node": "8.3.0",
    "tslint": "5.19.0",
    "typescript": "3.5.3",
    "wait-on": "3.3.0",
    "zone.js": "0.9.1"
  },
  "browser": {
    "fs": false,
    "net": false,
    "tls": false
  }
}
