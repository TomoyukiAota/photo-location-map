{
  "productName": "Photo Location Map",
  "directories": {
    "output": "release/"
  },
  "files": [
    "**/*",
    "!**/*.ts",
    "!e2e/",
    "!package/",
    "!package-test/",
    "!release/",
    "!script/",
    "!src/",
    "!test/",
    "!type-declaration/",
    "!.editorconfig",
    "!.eslintrc.js",
    "!.gitignore",
    "!.mergify.yml",
    "!.travis.yml",
    "!angular.json",
    "!appveyor.yml",
    "!browserlist",
    "!electron-builder.json",
    "!LICENSE",
    "!package.json",
    "!package-lock.json",
    "!postcss.config.json",
    "!postinstall.js",
    "!postinstall-web.js",
    "!README.md",
    "!tsconfig.json",
    "!tsconfig-serve.json",
    "!tslint.json"
  ],
  "win": {
    "icon": "dist",
    "target": [
      "portable"
    ]
  },
  "mac": {
    "icon": "dist",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "icon": "dist",
    "target": [
      "AppImage"
    ]
  }
}
